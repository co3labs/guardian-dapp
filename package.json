  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "eject": "react-scripts eject",
    "clearTestCache": "react-scripts test --clearCache",
    "test-rinkeby-txs": "yarn run test-rinkeby-tradex && yarn run test-rinkeby-stakex && yarn run test-rinkeby-unstake",
    "test-rinkeby-standards": "yarn run test-rinkeby-cookies && yarn run test-rinkeby-disclaimer && yarn run test-rinkeby-lp",
    "test-rinkeby-tradex": "react-scripts test --preset @chainsafe/dappeteer src/tests/rinkeby/desktop/tradeX.test.ts",
    "test-ui": "yarn run test-uiNav && yarn run test-uiStakeX && yarn run test-uiTradeX && yarn run test-uiUnstake",
    "test-uiNav": "react-scripts test --preset @chainsafe/dappeteer src/tests/uiNav.test.ts",
    "test-uiStake": "react-scripts test --preset @chainsafe/dappeteer src/tests/uiStake.test.ts",
    "test-uiTrade": "react-scripts test --preset @chainsafe/dappeteer src/tests/uiTrade.test.ts",
    "test-uiUnstake": "react-scripts test --preset @chainsafe/dappeteer src/tests/uiUnstake.test.ts",
    "test-rinkeby-disclaimer": "react-scripts test --preset @chainsafe/dappeteer src/tests/rinkeby/desktop/disclaimer.test.ts",
    "test-rinkeby-stakex": "react-scripts test --preset @chainsafe/dappeteer src/tests/rinkeby/desktop/stakeX.test.ts",
    "test-rinkeby-lp": "react-scripts test --preset @chainsafe/dappeteer src/tests/rinkeby/desktop/liquidityPosition.test.ts",
    "test-rinkeby-cookies": "react-scripts test --preset @chainsafe/dappeteer src/tests/rinkeby/desktop/cookies.test.ts",
    "test-rinkeby-unstake": "react-scripts test --preset @chainsafe/dappeteer src/tests/rinkeby/desktop/unstake.test.ts",
    "coverage": "yarn test-rinkeby -- --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chainsafe/dappeteer": "^2.3.0",
    "@types/react-outside-click-handler": "^1.3.0",
    "@types/react-router-dom": "^5.3.3",
    "autoprefixer": "^9",
    "postcss": "^7",
    "puppeteer": "^13.0.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  }
